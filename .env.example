# Environment variables for the Amp Task Management UI

# Development
NODE_ENV=development

# Amp Orchestrator API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_WS_BASE_URL=ws://localhost:8080

# API Client Configuration
VITE_API_TIMEOUT=30000
VITE_WS_RECONNECT_INTERVAL=1000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10
VITE_WS_HEARTBEAT_INTERVAL=30000

# Development Features
VITE_ENABLE_DEV_TOOLS=true
VITE_LOG_LEVEL=debug

# Authentication (will be enabled later)
# VITE_AUTH_ENABLED=false

# Production Example:
# VITE_API_BASE_URL=https://your-amp-orchestrator.com
# VITE_WS_BASE_URL=wss://your-amp-orchestrator.com
# VITE_LOG_LEVEL=warn
# VITE_ENABLE_DEV_TOOLS=false
